{% extends 'base.html.twig' %}

{% block title %}Hello ProductController!{% endblock %}

{% block body %}
    <h1>Liste des marques</h1>

    <ul>
        {% for brand in brands %}
            <li>
                <a href="{{ path('app_brand_show', {'id': brand.id}) }}">{{ brand.brandName }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
